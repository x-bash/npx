___x_cmd_npx(){
    ___x_cmd___npx_origin "$@"
}

if command -v npx; then

    ___x_cmd___npx_origin(){
        command -v npx "$@"
    }

else

    x env try node=latest
    ___x_cmd___npx_origin(){
        command -v npx "$@"
    }

fi
